/* pages/my/complaints/complaints.wxss */
#complaints{
  width: 100%; height: 100%; background-color: #FFFFFF; box-sizing: border-box; padding: 0 40rpx; padding-top: 100rpx;
}

text{ font-size: 30rpx; color: #2B2B2B; font-family:PingFang-SC-Medium; font-weight:500; }
.section{
  width: 100%; height: 380rpx; background-color: #F6F6F6; margin-top: 30rpx; box-sizing: border-box;
  padding: 30rpx 20rpx;
}
textarea{
  width: 100%; height: 100%; font-size: 28rpx;
}

.complaints_success{
  width: 300rpx; height: 72rpx; border: 1rpx solid #C15356; background: none; color: #C15356; font-size: 36rpx;
  line-height: 72rpx; margin: 0 auto; border-radius: 36rpx; margin-top: 100rpx; text-align: center;
}